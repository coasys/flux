{
  "compilerOptions": {
    "jsx": "preserve",
    "jsxFactory": "h",
    "jsxFragmentFactory": "Fragment",
    "downlevelIteration": true,
    "skipLibCheck": true,
    "baseUrl": "/",
    "paths": {
      "react": ["node_modules/preact/compat"],
      "react-dom": ["node_modules/preact/compat"]
    }
  },
  "include": [
    "src/**/*.tsx",
    "src/**/*.ts"
  ]
}
