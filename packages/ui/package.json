{
  "name": "@fluxapp/ui",
  "version": "0.5.9",
  "description": "",
  "homepage": "https://docs.fluxsocial.io",
  "main": "dist/main.js",
  "browser": "dist/main.js",
  "types": "dist/main.d.ts",
  "files": [
    "dist"
  ],
  "publishConfig": {
    "cache": "~/.npm",
    "access": "public"
  },
  "scripts": {
    "prepare": "run-s build:*",
    "release": "np --no-2fa --no-tests --tag=latest",
    "release:beta": "np --no-2fa --no-tests --tag=beta",
    "start": "run-p dev:*",
    "build": "run-s build:*",
    "dev": "run-p dev:*",
    "dev:lib": "NODE_ENV=dev node esbuild.js",
    "build:types": "node buildTypes.js",
    "build:analyze": "web-component-analyzer src --outFile components.json",
    "build:lib": "NODE_ENV=production node esbuild.js"
  },
  "dependencies": {
    "@popperjs/core": "^2.9.2",
    "construct-style-sheets-polyfill": "^3.1.0",
    "jdenticon": "^3.1.0",
    "lit": "^2.0.0-rc.2"
  },
  "devDependencies": {
    "esbuild": "^0.12.0",
    "esbuild-plugin-copy": "^2.1.1",
    "esbuild-plugin-minify-html-literals": "^1.0.2",
    "np": "^7.5.0",
    "npm-run-all": "^4.1.5",
    "web-component-analyzer": "^1.1.6"
  },
  "keywords": [],
  "author": "",
  "license": "ISC"
}
