{
  "name": "flux-webrtc-debug-view",
  "version": "1.0.0",
  "description": "WebRTC-debugger for flux",
  "fluxapp": {
    "name": "DebugRTC",
    "description": "WebRTC-debugger for flux",
    "icon": "bug"
  },
  "keywords": [
    "flux-plugin",
    "ad4m-view"
  ],
  "type": "module",
  "files": [
    "dist"
  ],
  "main": "./dist/main.umd.cjs",
  "module": "./dist/main.js",
  "exports": {
    ".": {
      "import": "./dist/main.js",
      "require": "./dist/main.umd.cjs"
    }
  },
  "baseUrl": "./",
  "paths": {
    "react": [
      "./node_modules/preact/compat/"
    ],
    "react-dom": [
      "./node_modules/preact/compat/"
    ]
  },
  "scripts": {
    "start": "vite",
    "dev": "run-p dev:**",
    "dev:serve": "vite --host",
    "dev:watch": "vite build --watch",
    "build": "vite build",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "@coasys/flux-container": "*",
    "@coasys/flux-utils": "0.8.1",
    "@coasys/flux-webrtc": "*",
    "@coasys/ad4m-react-hooks": "0.10.0-rc2",
    "classnames": "^2.3.2",
    "date-fns": "^2.29.3",
    "emoji-picker-element": "^1.14.0",
    "hark": "^1.2.3",
    "howler": "^2.2.3",
    "npm-run-all": "^4.1.5",
    "preact": "^10.11.3"
  },
  "devDependencies": {
    "@preact/preset-vite": "^2.5.0",
    "vite": "^4.3.5",
    "vite-plugin-css-injected-by-js": "^2.3.1"
  }
}
