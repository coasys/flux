{
  "name": "flux-monorepo",
  "version": "0.5.9",
  "workspaces": {
    "packages": [
      "packages/**/*",
      "views/**/*",
      "app",
      "docs",
      "electron"
    ],
    "nohoist": [
      "**/electron",
      "**/electron/**",
      "**/electron-squirrel-startup",
      "**/electron-squirrel-startup/**",
      "**/@electron-forge",
      "**/@electron-forge/**",
      "**/electron-builder",      
      "**/electron-builder/**"
    ]
  },
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel",
    "lint": "turbo run lint"
  },
  "devDependencies": {
    "eslint-config-custom": "*",
    "prettier": "latest",
    "turbo": "latest"
  },
  "engines": {
    "node": ">=14.0.0"
  },
  "dependencies": {},
  "packageManager": "yarn@1.22.19"
}
