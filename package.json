{
  "name": "junto",
  "version": "0.1.9",
  "private": true,
  "description": "Junto ad4m chat application leveraging distributed & decentralized technologies",
  "author": "josh@junto.foundation",
  "repository": {
    "type": "git",
    "url": "https://github.com/juntofoundation/communities.git"
  },
  "scripts": {
    "lint": "vue-cli-service lint",
    "get-languages": "node scripts/get-languages.js",
    "build-languages": "npm run get-languages && for OUTPUT in $(ls ad4m/languages); do if [ $OUTPUT != \"profiles\" ]; then echo \"Building: $OUTPUT\" && cd ad4m/languages/$OUTPUT && npm i && npm run build && cd -; fi; done",
    "build-start": "cd ../ad4m-core-executor && npm run build && cd ../communities && npm run electron:serve",
    "clean-state": "node scripts/clean-state.js",
    "electron:build": "vue-cli-service electron:build",
    "electron:serve": "vue-cli-service electron:serve",
    "postinstall": "patch-package && electron-builder install-app-deps",
    "postuninstall": "electron-builder install-app-deps",
    "rebuild": "./node_modules/.bin/electron-rebuild -f -w ad4m-core-executor",
    "build-holochain": "echo \"\n\n\n\nThis next stage will fail if Nix is not installed!! You can install here: https://nixos.wiki/wiki/Nix_Installation_Guide\" && sleep 5 && nix-shell --run ./scripts/copy-hc.sh",
    "release": "npm run electron:build",
    "test": "jest --env=jsdom",
    "test-gql": "jest --env=jsdom src/__tests__/graphql.ts"
  },
  "main": "background.js",
  "dependencies": {
    "@apollo/client": "^3.3.14",
    "@junto-foundation/junto-elements": "^0.2.12",
    "@perspect3vism/ad4m": "0.1.4",
    "@perspect3vism/ad4m-executor": "file:../ad4m-executor",
    "@tiptap/core": "^2.0.0-beta.85",
    "@tiptap/vue-3": "^2.0.0-beta.45",
    "@types/jest": "^26.0.20",
    "@types/mocha": "^8.2.1",
    "@vue/apollo-composable": "^4.0.0-alpha.12",
    "@vue/apollo-util": "^4.0.0-alpha.11",
    "core-js": "^3.6.5",
    "date-fns": "^2.22.1",
    "electron-updater": "^4.3.9",
    "esm": "^3.2.25",
    "graphql": "^15.5.0",
    "object-hash": "^2.2.0",
    "patch-package": "^6.4.7",
    "register-service-worker": "^1.7.1",
    "subscriptions-transport-ws": "^0.9.18",
    "uuid": "^8.3.2",
    "vue": "^3.0.0",
    "vue-advanced-cropper": "^2.3.1",
    "vue-router": "^4.0.0-0",
    "vue3-virtual-scroller": "^0.2.2",
    "vuex": "^4.0.0",
    "vuex-persist": "^3.1.3",
    "webpack-node-externals": "^2.5.2"
  },
  "devDependencies": {
    "@babel/preset-typescript": "^7.14.5",
    "@testing-library/vue": "^6.4.2",
    "@types/electron-devtools-installer": "^2.2.0",
    "@types/uuid": "^8.3.0",
    "@typescript-eslint/eslint-plugin": "^4.28.1",
    "@typescript-eslint/parser": "^4.28.1",
    "@vue/cli-plugin-babel": "~4.5.0",
    "@vue/cli-plugin-eslint": "~4.5.0",
    "@vue/cli-plugin-pwa": "~4.5.0",
    "@vue/cli-plugin-router": "~4.5.0",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "^5.0.0-beta.2",
    "@vue/cli-plugin-vuex": "~4.5.0",
    "@vue/cli-service": "~4.5.0",
    "@vue/compiler-sfc": "^3.0.0",
    "@vue/eslint-config-prettier": "^6.0.0",
    "@vue/eslint-config-typescript": "^7.0.0",
    "babel-jest": "^26.0.0",
    "easygraphql-tester": "^6.0.1",
    "electron": "^11.4.7",
    "electron-devtools-installer": "^3.1.0",
    "electron-rebuild": "^2.3.5",
    "eslint": "^6.7.2",
    "eslint-plugin-prettier": "^3.3.1",
    "eslint-plugin-vue": "^7.12.1",
    "fs-extra": "^10.0.0",
    "jest": "^26.0.0",
    "jest-transform-stub": "^2.0.0",
    "node-sass": "^4.12.0",
    "node-wget-js": "^1.0.1",
    "prettier": "^2.3.2",
    "react": "^17.0.2",
    "sass-loader": "^8.0.2",
    "ts-jest": "^26.0.0",
    "typescript": "^4.2.4",
    "vue-cli-plugin-electron-builder": "~2.0.0-rc.6",
    "vue-devtools": "^5.1.4",
    "vue-jest": "^5.0.0-alpha.10"
  },
  "contributors": [
    "eric@junto.foundation"
  ]
}
