{
  "name": "flux-monorepo",
  "version": "5",
  "workspaces": [
    "packages/**/*",
    "views/**/*",
    "app"
  ],
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev --parallel --ignore='./packages/ui/.vitepress/cache/**/*'",
    "lint": "turbo run lint"
